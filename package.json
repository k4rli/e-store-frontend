{
	"name": "erply-e-store",
	"version": "0.1.0",
	"private": true,
	"description": "E-store frontend",
	"homepage": "https://karliyo.github.io/e-store-frontend/",
	"scripts": {
		"start": "nodemon --exec \"cross-env NODE_ENV=development webpack-dev-server -d --progress --colors --config bundle/webpack.dev.js\"",
		"prebundleDISABLED": "npm run test",
		"bundle": "cross-env NODE_ENV=production webpack -p --config bundle/webpack.prod.js --hide-modules --display-optimization-bailout",
		"lint": "eslint src --fix",
		"test": "jest",
		"test-watch": "jest --watch"
	},
	"devDependencies": {
		"@babel/core": "7.5.5",
		"@babel/plugin-proposal-class-properties": "7.5.5",
		"@babel/plugin-proposal-function-bind": "7.2.0",
		"@babel/plugin-proposal-optional-chaining": "7.2.0",
		"@babel/plugin-transform-classes": "7.5.5",
		"@babel/plugin-transform-runtime": "7.5.5",
		"@babel/preset-env": "7.5.5",
		"@babel/preset-react": "7.0.0",
		"@hot-loader/react-dom": "16.8.6",
		"@testing-library/react": "8.0.7",
		"axios-mock-adapter": "1.17.0",
		"babel-eslint": "10.0.2",
		"babel-loader": "8.0.6",
		"babel-plugin-module-resolver": "3.2.0",
		"connect-history-api-fallback": "1.6.0",
		"cross-env": "5.2.0",
		"css-loader": "3.1.0",
		"depcheck": "0.8.3",
		"enzyme": "3.10.0",
		"enzyme-adapter-react-16": "1.14.0",
		"eslint": "6.1.0",
		"eslint-config-airbnb": "17.1.1",
		"eslint-import-resolver-babel-module": "5.1.0",
		"eslint-plugin-import": "2.18.2",
		"eslint-plugin-jest": "22.13.7",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-react": "7.14.3",
		"eslint-plugin-react-hooks": "1.6.1",
		"friendly-errors-webpack-plugin": "1.7.0",
		"html-webpack-plugin": "3.2.0",
		"http-proxy-middleware": "0.19.1",
		"jest": "24.8.0",
		"mini-css-extract-plugin": "0.8.0",
		"node-sass": "4.12.0",
		"nodemon": "1.19.1",
		"optimize-css-assets-webpack-plugin": "5.0.3",
		"redux-mock-store": "1.5.3",
		"redux-thunk": "2.3.0",
		"sass-loader": "7.1.0",
		"sinon": "7.3.2",
		"style-loader": "0.23.1",
		"terser-webpack-plugin": "1.3.0",
		"webpack": "4.38.0",
		"webpack-bundle-analyzer": "3.3.2",
		"webpack-cli": "3.3.6",
		"webpack-dev-server": "3.7.2",
		"webpack-merge": "4.2.1"
	},
	"dependencies": {
		"@babel/runtime": "7.5.5",
		"copy-webpack-plugin": "5.0.4",
		"core-js": "3.1.4",
		"js-file-download": "0.4.7",
		"react": "16.8.6",
		"react-dom": "16.8.6",
		"react-hot-loader": "4.12.9",
		"react-router": "5.0.1",
		"react-router-dom": "5.0.1",
		"regenerator-runtime": "0.13.3",
		"use-dark-mode": "2.3.1"
	},
	"jest": {
		"setupFilesAfterEnv": [
			"<rootDir>src/test/jestSetup.js"
		],
		"moduleFileExtensions": [
			"js",
			"jsx",
			"json"
		],
		"moduleNameMapper": {
			"\\.(css|scss)$": "<rootDir>src/test/emptyMock.js"
		}
	}
}
